@import _mixins.sass
@import _variables.sass

/**************************
/*  PAGE
/**************************/
#page
  overflow: hidden


/**************************
/*  PAGE ELEMENTS
/**************************/

.page-element

  /* site name + slogan
  &.site-name-wrapper,
  &.site-slogan-wrapper
    display: none



/**************************
/*  HEADER SECTION
/**************************/

#section-header


  /*-------------------------
  /*  User zone
  /*-------------------------

  #zone-user-wrapper
    background: $blue
    
  #zone-user
    .region
      min-height: $zone_user_wrapper_h
    
    /*  User First Region
    /*.........................
    #region-user-first
      display: none
    
    /*  User Second Region
    /*.........................
    #region-user-second
  
 
  /*-------------------------
  /* Branding Zone
  /*-------------------------
 
  #zone-branding-wrapper
    background: $blue_sky
  
  #zone-branding
  
    /*  Zone Branding Region
    /*.........................
    
    #region-branding
      height: $zone_branding_wrapper_h
    
  /*-------------------------
  /*  Menu zone
  /*-------------------------

  #zone-menu-wrapper
    display: none
    
  #zone-menu
  
    /*  Menu Region
    /*.........................
    #region-menu
    
    
    
/**************************
/*  CONTENT SECTION
/**************************/

#section-content

  /*-------------------------
  /*  Preface zone
  /*-------------------------

  #zone-preface-wrapper
    background: url(../images/GreyStripesDarker.png) repeat
    border-bottom: 2px solid $grey_light
    border-top: 2px solid $grey_light
    min-height: $region_preface_wrapper_h

  
  #zone-preface
    
    /*  Preface First Region
    /*.........................
    #region-preface-first
      display: none

    
    /*  Preface Second Region
    /*.........................
    #region-preface-second
      display: none
    
    /*  Preface Third Region
    /*.........................
    #region-preface-third
      height: $region_preface_third_h
      padding-top: 19px
      margin-bottom: -2px
      
   
  /*-------------------------
  /*  Header zone
  /*-------------------------

  #zone-header-wrapper

  #zone-header
    
    /*  Header First Region
    /*.........................
    #region-header-first

    
    /*  Header Second Region
    /*.........................
    #region-header-second   
      
 
  /*-------------------------
  /*  Content zone
  /*-------------------------

  #zone-content-wrapper
    padding-bottom: 45px
    padding-top: 10px

  #zone-content
    
    /*  Sidebar First Region
    /*.........................
    #region-sidebar-second

    
    /*  Content Region
    /*.........................
    #region-preface-second
    
    
    /*  Sidebar Second Region
    /*.........................
    #region-sidebar-second
      padding-top: 40px
    
  /*-------------------------
  /*  Postscript zone
  /*-------------------------

  #zone-postscript-wrapper
    background: url(../images/GreyStripesDarker.png) repeat
    border-top: 2px solid $grey_light
    

  #zone-postscript
    
    /*  Postscript First Region
    /*.........................
    #region-postscript-first

    
    /*  Postscript Second Region
    /*.........................
    #region-postscript-second
    
    
    /*  Postscript Third Region
    /*.........................
    #region-postscript-third
    
    /*  Postscript Fourth Region
    /*.........................
    #region-postscript-fourth
      clear: both
      
    
    
    
/**************************
/*  FOOTER SECTION
/**************************/

#section-footer
  background: $blue
  
  &,
  a
    color: #fff

  /*-------------------------
  /*  Footer zone
  /*-------------------------

  #zone-footer-wrapper

  #zone-footer
    min-height: 40px
    padding-bottom: 10px
    font-size: 1em
    line-height: 1.45em
    
    /*  Footer First Region
    /*.........................
    #region-footer-first

      
      
/**************************
/* MAIN MENU
/**************************/
      
@media all and (max-width: 599px)
  #block-superfish-1
    .block-inner
      color: #fff
      height: $zone_user_wrapper_h
      overflow: hidden
      padding-top: 0
      position: relative
          
      &.menu-open
        h2.block-title
          
      .content
        position: absolute
        width: 100%
        top: 60px
    
    h2.block-title
      +block-link(80px, $zone_user_wrapper_h, 24px)
      +icons-sprite(menu, false, 80 - (icons-sprite-width(menu)), center-y($zone_user_wrapper_h, menu))
      color: #fff
      margin-bottom: 0
      padding-right: 10px
      position: absolute
      right: 0
      top: 0
      z-index: 999   
      
      &:hover
        cursor: pointer
        
    ul.main-menu.sf-menu
      width: 100%
      
      li.sf-depth-1
        width: 33%
     
        a.sf-depth-1
          padding: 0
          text-align: center
      

ul.main-menu.sf-menu
  //height: $zone_menu_wrapper_h
  margin-bottom: 0
  //margin-top: -60px
  
  li
    background: none
    
    &:hover
      background: none
    
    a,
    a:visited
      +block-link(auto, auto, 24px)
      border: none
      color: #fff
      text-transform: uppercase
      
      &:hover,
      &.active
        background: none
        color: #3dbbf4
        text-decoration: none 
    
    //top level
    &.sf-depth-1
      
      a.sf-depth-1
        //+box-border(auto, $zone_menu_wrapper_h, $main_menu_border)
        display: block
        height: $zone_user_wrapper_h
        line-height: $zone_user_wrapper_h
        padding: 0 25px
        text-align: left
        
        &:hover,
        &.active
        
        //home item  
        &#sf-main-menu-home
          
          &:hover
            //background-position: center -60px
      
      &.last
        a.sf-depth-1
          border-right: 0
          
      &.first
        display: block
        
        a.sf-depth-1 
      
      &.sfHover a.sf-depth-1,
      a.sf-depth-1:hover
        
      &.active-trail
        a.sf-depth-1
        
    
/**************************
/*  NODES
/**************************/   
.node

  /*-------------------------
  /*  Page node
  /*-------------------------
  &.node-page
  
  /*-------------------------
  /*  Full node
  /*-------------------------
  &.node-full
   
   
/**************************
/*  MULTI BLOG NODES + ENTITIES
/**************************/ 

.ds-2col
  position: relative
  
  /* Standard set display suit to full
  .group-left,
  .group-right
    clear: both
    width: 100%
  
  /* tags   
  .field-name-field-tags
    .field-label,
    .field-item
      //padding-right: 0.5em
     
  .media-embed-resize 
    .media-embed-wrapper
      .media-embed-wrapper
        padding-top: 0!important
        position: static
      
      .media-embed-wrapper-outer
        max-height: none!important
        max-width: none!important
  
  .field-type-text-with-summary,
  .field-type-media,
  .field-type-image,
  .field-name-reach-media-galleria-slideshow,
  .field-type-text-long
    margin-bottom: 20px
    
  .field-name-media-gallery-media
    .field-item,
    .galleria-content,
    .galleria-stage
      width: 100%
      
      .galleria-errors,
      .galleria-fullscreen,
      .galleria-s4
        display: none
        
      .galleria-popout
        right: 0
      
    
  /* Galleria width issue
  .field-name-reach-media-galleria-slideshow,
    .galleria-errors,
    .galleria-fullscreen,
    .galleria-s4
      display: none
      
    .galleria-popout
      right: 0
    
    .galleria-thumbnails-container
      .galleria-thumbnails
        height: auto!important
        width: auto!important
        
        .galleria-image
          height: 80px!important
          width: 80px!important
          overflow: hidden!important
     
    /*  
      .galleria-container
        max-width: 100%!important
      
      /*
        img
        max-width: 100%!important
        height: auto!important
  
  /*  hide oath title  
  a.oembed-title
    display: none
    
  .oembed-content
    display: block
    width: 100%
    
  .field-name-author-with-by-
    margin-bottom: 15px
    font-style: italic
  
  /* post date 
  .field-name-post-date
    +adelle(bolditalic)
    +block-link(auto, auto, 14px, false)
    color: $text_color_alt
    margin-bottom: 1em
        
  /*-------------------------
  /*  Teaser node
  /*-------------------------
  &.node-teaser
    margin-bottom: 3em
    
    .views-row-last &
      .main-display
        border-bottom: 0
        
        
  /*-------------------------
  /*  Full node
  /*-------------------------
  &.view-mode-full
    .group_links
      margin-bottom: 35px
    
  /*-------------------------
  /*  Blog node
  /*-------------------------
  &.node-blog
    min-height: 270px
    
  /*-------------------------
  /*  Blog node
  /*-------------------------
  &.node-media-gallery
    .action-links
      ul,li
        padding: 0
        margin: 0
    
  /*-------------------------
  /*  Multi Blog nodes
  /*-------------------------
  &.node-blog,
  &.node-video,
  &.node-media-gallery
    .field-type-media
      .field-name-media-title,
      .field-name-field-license
        display: none
        
    .group-left
      display: none
      
    .action-links
      
      
/* 3 column layout
.ds-3col
  &.user-profile
    .field-name-field-name
      +adelle(bold)
      +block-link(auto, auto, 24px, false)
      color: $text_color_alt
      margin-bottom: 1em
      
  .field-name-ds-user-picture
    img
      display: block
      margin: 0 auto
      
  .field-name-author-anchor
    height: 20px
    margin-top: -20px
      
  .group-left,
  .group-middle,
  .group-right
    clear: both
    width: 100%
    
  .group-middle,
  .group-right
    padding-top: 40px
    

/* for image file displays
.media-gallery-detail-wrapper
  .media-gallery-detail
    float: none
    margin-right: 0
    width: 100%
    max-width: none
    
  .field-name-reach-social-links-block-
    margin-bottom: 35px
    
  .no-overflow
    clear: both
    margin-top: 35px


/**************************
/*  MULTI BLOG NODES + FILES
/**************************/     
.ds-2col,
.media-gallery-detail-wrapper
  
  .field-name-reach-social-links-block-,
  .group_links
    background: url(../images/GreyStripesDarker.png) repeat
    height: 43px
  
  /* links social + others    
  ul.links
    display: inline-block
    padding-top: ((43px - 24px) / 2)
    width: 100%
    
    li
      display: block
      float: left
      font-style: italic
      
      &.facebook,
      &.twitter,
      &.google
        margin: 0
        padding: 0
        width: 33%
        text-align: center
        
        a
          +sprite-block(grid-width(1), 24px, 0, 0, "socialbuttonsprite.png")
          display: inline-block
        
      &.facebook
        a
          &:hover
            background-position: 0 -24px
            
      &.twitter
        a
          background-position: 0 -48px
          
          &:hover
            background-position: 0 -72px
        
      &.google
        a
          background-position: 0 -96px
          
          &:hover
            background-position: 0 -120px
            
      &.node-readmore,
      &.comment_count
        display: none  

/* facebook
.field-name-reach-social-blocks-fbcomments
  display: none      

.view-id-galleria_slideshow
  overflow: auto
  
  .attachment-before,
  .view-content
    float: left
    
  .attachment-before
    
  .view-content
    height: 100%
    width: 100%
    
    .skin-default
      height: auto!important
      height: 100%
      min-height: 100%
    
    .views-slideshow-galleria-main-frame
      height: 100%
      width: 100%
      
  
      a:first-child
        img
          display: block
      
.js
  .view-id-galleria_slideshow
    .attachment-before
    
  
    
/**************************
/*  VIEWS
/**************************/    
.view
  .views-row
    &.views-row-first
      .node-teaser
        margin-top: 0
        
  /* blog listing
  &.view-multi-content-blog
    .pager-load-more
      a
        +buttonize-link(120px, 40px, 24px)
        margin: 0 auto
        //width: 40%
  
  /* authors page listing
  &.view-content-authors     
    .views-row
      padding-top: 35px
      border-bottom: 2px solid #d9d8d1
      
      .group-right
      
      
      &.views-row-first
        padding-top: 0
      
      &.views-row-last
        border-bottom: 0

        
  /* more mosts views      
  &.view-multi-content-blog      
    &.view-display-id-block_1,
    &.view-display-id-block_2   
      .views-row,
      .view-empty
        padding: 25px 0
        font-style: italic
        line-height: 1.25em
        color: $orange_dark
      
      .views-row
        border-bottom: 2px solid #d9d8d1
        
        &.views-row-last
          border-bottom: 0


/**************************
/* MORE POSTS BLOCKS
/**************************/
  
  /* more posts block
.block-views-multi-content-blog-block-1,
.field-name-reach-multi-content-blog-latest
  h2.block-title
    +block-link(auto, auto, 30px)
    font-weight: normal
    font-style: normal
    margin-bottom: 0
    color: #d0cec6
    line-height: 30px
       

/**************************
/*  BLOCKS
/**************************/

.block
  
  /*-------------------------
  /*  Header image block
  /*-------------------------
  &.block-random-banner-block
    position: absolute
    height: $zone_branding_wrapper_h
    overflow: hidden
    right: -50%
    top: 0
    z-index: 0
    
    .views-field-field-banner-images
      img
        max-height: $zone_branding_wrapper_h
    
    
    
  /*-------------------------
  /*  Custom Blocks
  /*-------------------------
  &.block-block
    
    // back to aidscycle link
    &.block-block-5
      margin-left: 205px
      
      a, a:hover
        color: #fff
        line-height: $zone_user_wrapper_h
        
      a:hover
        text-decoration: underline
  
    /* you belong here text + more about the ride link
    &#block-block-3,
    &#block-block-4
      .content
        +adelle(bold)
        text-align: center
    
    /* you belong here text
    &#block-block-3
      
      .content
        color: #fff
        font-size: 30px
        line-height: 34px
        padding: 20px 0 15px 0
        text-shadow: #CFCDC5 0.06em 0.06em 0
        //filter: 'shadow(color=#E7E5dA,direction=135,strength=5)', 'progid:DXImageTransform.Microsoft.AlphaImageLoader(src="../images/hIEfix.png",sizingMethod=crop)'
        //-ms-filter: 'progid:DXImageTransform.Microsoft.Shadow(color=#E7E5dA,direction=135,strength=5), progid:DXImageTransform.Microsoft.AlphaImageLoader(src="../images/hIEfix.png",sizingMethod=crop)'
    
    /* more about the ride link
    &#block-block-4
      
      .content
        font-size: 18px
        padding: 20px 0
        
        a
          color: $orange_dark
   
   
    &.block-block-2
      margin-bottom: 0
      
      a
        font-weight: bold
      
  
  /*-------------------------
  /*  Button Blocks
  /*-------------------------
  &.block-bean
    // bean buttons
    .bean-button-block
      a
        +buttonize-link(120px, 40px, 24px)
    
    /* donate and register    
    &.block-donate-button,
    &.block-register-button
      float: left
      width: 38%
      
      .bean-button-block
        a
          width: 100%
          
      /* donate and register in branding
      &.block-region-branding
        margin-top: 170px
        position: relative
    
    /* register
    &.block-register-button
      margin-right: 4%
      margin-left: 10%
        
        
    // alternate colors for donate 
    &.block-donate-button
      
      .bean-button-block
        a
          background-color: $orange
          border-bottom-color: darken($orange, 10%)
          
          &:hover
            background-color: lighten($orange, 5%)
      
    &.block-site-copyright-info
      padding-top: 10px
      margin-bottom: 10px
      
      .block-inner
        padding-right: 25px
    
      .entity-bean
        div
          display: inline
          
        .field-name-field-start-date,
        .field-name-current-year,
        .field-name-field-copyright-title, 
        .field-name-field-phone-number
          margin-right: 0.35em
          
        .field-name-field-copyright-title, 
        .field-name-field-phone-number
          border-right: 1px solid #FFF
          
          .field-items
            margin-right: 0.35em
    
        
  /*-------------------------
  /*  Blog filter block
  /*-------------------------
    
  &.block-reach-blog-filters
    bottom: -2px
    clear: both
    margin: 0 auto
    width: 250px
    
    ul
      +list-float()
      border-top: 2px solid $grey_light
      
      li
        margin: 0
        text-align: center
        
        &.last
          a
            border-right: 2px solid $grey_light
      
      a
        background-color: $grey_faint!important
        +block-link(60px, 50px)
        border-bottom: 2px solid $grey_light
        border-left: 2px solid $grey_light
        color: $text_color_alt
        text-indent: -9000px
        
        &.current
          background-color: #fff!important
          border-bottom-color: #fff
          
        &.view-type-All
          +icons-sprite(everything_tab, false, center-x(60px, everything_tab), center-y(50px, everything_tab))
        
        // blog
        &.view-type-blog
          +icons-sprite(blog_tab, false, center-x(60px, blog_tab), center-y(50px, blog_tab))
          
        // gallery
        &.view-type-media_gallery
          +icons-sprite(image_tab, false, center-x(60px, image_tab), center-y(50px, image_tab))
        
        // video 
        &.view-type-video
          +icons-sprite(video_tab, false, center-x(60px, video_tab), center-y(50px, video_tab))
          
          
          
          
