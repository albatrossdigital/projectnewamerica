// Sprite creation
@import "compass/utilities/sprites/base"
$icons-spacing: 60px
@import "icons/*.png"
$disable-magic-sprite-selectors:true
@include all-icons-sprites


// Sprite mixins

@function center-y($y, $item)
  $h: icons-sprite-height($item)
  @return (($y - $h) / 2)
  
@function center-x($x, $item)
  $w: icons-sprite-width($item)
  @return (($x - $w) / 2)
  
  

//****
// FONTs

@mixin adelle($alt: normal)
  font-family: $font_adelle
  @if $alt == italic
    font-style: italic
  @else if $alt == bold
    font-weight: bold
  @else if $alt == bolditalic
    font-style: italic
    font-weight: bold
  
@mixin leaguegothic()
  font-weight: normal
  font-family: $font_league

// Borderbox mixin
// Use box-sizing: border-box; with impunity
// Borderbox outputs equivalent CSS in <IE8

// Set to true if you prefer declaring a global box-sizing prop.
//
// Ex:
// $global-border-box: true;
// 
// * {
//     @include box-sizing(border-box);
// }

$global-border-box: false !default

@mixin borderbox($dimensions, $padding: false, $border: false) 
  @if not $global-border-box 
    @include box-sizing(border-box)
  

  // Extract width/height values
  $width: nth($dimensions, 1)
  $height: false

  @if length($dimensions) == 2 
    $height: nth($dimensions, 2)
  

  // Extract padding values
  $padding-length: length($padding)
  $padding-y: 0
  $padding-x: 0

  @if $padding 
    padding: $padding

    @if $padding-length == 4 
      $padding-y: nth($padding, 1) + nth($padding, 3)
      $padding-x: nth($padding, 2) + nth($padding, 4)
    @else if $padding-length == 3 
      $padding-y: nth($padding, 1) + nth($padding, 3)
      $padding-x: nth($padding, 2) * 2
    @else if $padding-length == 2 
      $padding-y: nth($padding, 1) * 2
      $padding-x: nth($padding, 2) * 2
    @else if $padding-length == 1 
      $padding-y: $padding * 2
      $padding-x: $padding * 2
    
  

  // Extract border values
  $use-border-shorthand: false
  $border-width: false

  @each $unit in $border 
    @if type-of($unit) == number 
      @if $border-width 
        $border-width: append($border-width, $unit)
      @else 
        $border-width: $unit
      
    @else 
      $use-border-shorthand: true
    
  

  $border-length: length($border-width)
  $border-y: 0
  $border-x: 0

  @if $border 
    @if $use-border-shorthand 
      border: $border
    @else 
      border-width: $border-width
    

    @if $border-length == 4 
      $border-y: nth($border-width, 1) + nth($border-width, 3)
      $border-x: nth($border-width, 2) + nth($border-width, 4)
    @else if $border-length == 3 
      $border-y: nth($border-width, 1) + nth($border-width, 3)
      $border-x: nth($border-width, 2) * 2
    @else if $border-length == 2 
      $border-y: nth($border-width, 1) * 2
      $border-x: nth($border-width, 2) * 2
    @else if $border-length == 1 
      $border-y: $border-width * 2
      $border-x: $border-width * 2
  

  // Set width value
  @if $width 
    @if $width == auto 
      width: $width
    @else 
      width: $width
      @if $legacy-support-for-ie 
        *width: $width - $padding-x - $border-x
      
    
  

  // Set height value
  @if $height 
    @if $height == auto 
      height: $height 
    @else
      height: $height
      @if $legacy-support-for-ie
        *height: $height - $padding-y - $border-y

@function grid-width($n, $left_outer: false, $right_outer: false)
  @if $n == false
    @return auto
  @else
    $width: ($n * $col_width) + (2 * $col_margin * ($n - 1))
    @if $left_outer == true
      $width: $width + $col_margin_outer
    @if $right_outer == true
      $width: $width + $col_margin_outer
    @return $width
    
@mixin rounded($r)
  +border-radius($r)
  border-radius: $r
  behavior: url(/sites/all/libraries/css3pie/PIE.htc)

// Utilize the themes alternate text in style
@mixin alternate-font($set: true, $f_weight: $font_alt_weight)
  +leaguegothic()
  @if $set == true
    font-style: normal
    letter-spacing: $font_league_spacing
    text-transform: uppercase
  
@mixin normal-font($set: true, $f_weight: normal)
  font-family: $font_normal
  @if $set == true
    font-weight: $f_weight
    text-transform: none

@mixin box-border($w, $h, $b)
  +box-sizing(border-box)
  border: $b solid
  //if height is auto
  @if $h == auto
    height: $h
  @else 
    height: $h
    *height: $h - (2 * $b)
  //if width is auto
  @if $w == auto 
    width: $w
  @else
    width: $w
    *width: $w - (2 * $b)
    
@mixin border-override($c: transparent)
  border-bottom-color: $c
  border-left-color: $c
  border-right-color: $c
  border-top-color: $c

@mixin block-link($w, $h, $f: 1em, $alt: true, $display: block)
  display: $display
  height: $h
  font-size: $f
  @if $h == auto
    line-height: 1em
  @else  
    line-height: $h
  @if $alt == true
    +alternate-font()
  text-decoration: none
  width: $w
  
  &:hover
    text-decoration: none
  
//Make link into button style
@mixin buttonize-link($w, $h, $f: 1.4em, $back_c: $b_back_color, $alt: true, $display: block)
  +block-link($w, $h, $f, $alt, $display)
  background: $b_back_color
  border-bottom: 2px solid darken($back_c, 20%)
  color: $color1_font
  +rounded(2px)
  text-align: center
  
  &:hover
    background: lighten($back_c, 5%)
  

//cross browser top facing fade
@mixin simple-grad-top($c, $c2: false)
  background-color: $c
  $experimental-support-for-svg: true
  //if no second color, just use lighten
  @if $c2 == false
    @include filter-gradient(lighten($c, 5%), $c)
    +background-image(linear-gradient(color-stops(lighten($c, 5%), $c 50%, $c 100%)))
  @else
    @include filter-gradient($c, $c2)
    +background-image(linear-gradient(color-stops($c, $c2 50%, $c2 100%)))  
  
//cross browser bottom facing fade
@mixin simple-grad-bottom($c, $c2: false)
  background-color: $c
  //if no second color, just use darken
  @if $c2 == false
    @include filter-gradient($c, darken($c, 5%))
    $experimental-support-for-svg: true
    +background-image(linear-gradient(color-stops($c, $c 50%, darken($c, 5%) 100%)))
  @else
    @include filter-gradient($c, $c2)
    $experimental-support-for-svg: true
    +background-image(linear-gradient(color-stops($c, $c 50%, $c2)))
  
@mixin square-button-link($n: false, $h: $square_block_h, $font_s: $square_block_font)
  $w: grid-width($n)
  display: block
  font-size: 1em
  height: $h
  position: relative
  width: $w
  
  span
    @if $w == auto
      +block-link($w, auto, $font_s)
    @else
      +block-link(($w - 20), auto, $font_s)
    bottom: 10px
    display: block
    position: absolute
    left: 10px
    
@mixin clear-square-button-link()
  float: none
  height: auto
  position: relative
  width: auto
  
  span
    bottom: auto
    left: 20px
    padding: 10px 0
    position: relative
    width: auto

    
