@import _mixins.sass
@import _variables.sass

/**************************
/*  PAGE ELEMENTS
/**************************/

.page-element
      
  &.logo-wrapper
    left: 50px
        
    a
      display: block
      height: auto
      width: auto
      
  /* site name + slogan
  &.site-name-wrapper,
  &.site-slogan-wrapper
    display: block
    
  /*page title  
  &.page-title-wrapper
    height: 75px
    margin-bottom: 7px
    padding-top: 18px

    
/* page title logos
body.not-front
  #region-sidebar-second
    //margin-top: 100px

body.node-type-media-gallery,
body.page-media-gallery-detail,
body.node-type-blog,
body.node-type-video,
body.context-about,
body.context-authors,
body.context-user
  #page-title
    padding-left: 92px
  

body.context-authors,
body.context-user
  .page-title-wrapper
  
  #page-title
    +icons-sprite(authors_tag_blue, false, 9px, 2px)
 
    
body.node-type-media-gallery,
body.page-media-gallery-detail
  #page-title
    +icons-sprite(image_tag_blue, false, 9px, 2px)
    
body.node-type-blog
  #page-title
    +icons-sprite(blog_tag_blue, false, 9px, 2px)
    
body.node-type-video
  #page-title
    +icons-sprite(blog_tag_blue, false, 9px, 2px)
  
body.context-about
  #page-title
    +icons-sprite(about_tag_blue, false, 9px, 2px)
        
          
/**************************
/*  PAGE
/**************************/
#page


/**************************
/*  HEADER SECTION
/**************************/

#section-header


  /*-------------------------
  /*  User zone
  /*-------------------------

  #zone-user-wrapper

  #zone-user
    .region
    
    /*  User First Region
    /*.........................
    #region-user-first
      display: block
    
    /*  User Second Region
    /*.........................
    #region-user-second
  
 
  /*-------------------------
  /* Branding Zone
  /*-------------------------
 
  #zone-branding-wrapper
    overflow: visible
  
  #zone-branding
  
    /*  Zone Branding Region
    /*.........................
    
    #region-branding
      height: $zone_branding_wrapper_h + 17
    
  /*-------------------------
  /*  Menu zone
  /*-------------------------

  #zone-menu-wrapper
    
  #zone-menu
  
    /*  Menu Region
    /*.........................
    #region-menu
    
    
    
/**************************
/*  CONTENT SECTION
/**************************/

#section-content

  /*-------------------------
  /*  Preface zone
  /*-------------------------

  #zone-preface-wrapper
  
  #zone-preface
    
    /*  Preface First Region
    /*.........................
    #region-preface-first
      display: block
    
    /*  Preface Second Region
    /*.........................
    #region-preface-second
      display: block
    
    /*  Preface Third Region
    /*.........................
    #region-preface-third
    
    
  /*-------------------------
  /*  Header zone
  /*-------------------------

  #zone-header-wrapper

  #zone-header
    
    /*  Header First Region
    /*.........................
    #region-header-first
      margin-left: 32px
      width: 712px
    
    /*  Header Second Region
    /*.........................
    #region-header-second
    

 
  /*-------------------------
  /*  Content zone
  /*-------------------------

  #zone-content-wrapper
    padding-top: 18px
    padding-bottom: 75px

  #zone-content
    
    /*  Sidebar First Region
    /*.........................
    #region-sidebar-first

    
    /*  Content Region
    /*.........................
    #region-content
      .region-inner
        padding-left: 22px
        
      &.grid-9
        width: 738px
        
        .region-inner
          padding-right: 4px

      &.grid-12
        .region-inner
          padding-right: 22px
    
    
    /*  Sidebar Second Region
    /*.........................
    #region-sidebar-second
      display: block
      padding-top: 0
    
      &.grid-3
        width: 182px
        
        .region-inner
          padding-right: 22px
    
  /*-------------------------
  /*  Postscript zone
  /*-------------------------

  #zone-postscript-wrapper

  #zone-postscript
    
    /*  Postscript First Region
    /*.........................
    #region-postscript-first

    
    /*  Postscript Second Region
    /*.........................
    #region-postscript-second
    
    
    /*  Postscript Third Region
    /*.........................
    #region-postscript-third
    
    /*  Postscript Fourth Region
    /*.........................
    #region-postscript-fourth


  /*-------------------------
  /*  Footer zone
  /*-------------------------

  #zone-footer-wrapper

  #zone-footer
    padding-bottom: 10px
    

/**************************
/* MAIN MENU
/**************************/


#block-superfish-1
  .block-inner
    width: auto!important

  h2.block-title
    display: none
  
  .content
    margin-left: 0
    
  .menu-open 
    .content
      border: none

ul.main-menu.sf-menu
  float: right
  overflow: visible
    
  li
    float: left
    
    &.sf-depth-1
      display: block!important
    
    a
     
    
    //top level
    &.sf-depth-1
      display: block
      
      
      a.sf-depth-1
        display: inline-block
        text-align: center
        
        &:hover,
        &.active
      
      &.last
        a.sf-depth-1
          
      &.first
        a.sf-depth-1 
      
      &.sfHover a.sf-depth-1,
      a.sf-depth-1:hover
        
      &.active-trail
        a.sf-depth-1
    
              
/**************************
/*  NODES
/**************************/   
.node
        
  /*-------------------------
  /*  Full node
  /*-------------------------
  &.node-full
  
  /*-------------------------
  /*  Teaser node
  /*-------------------------
  &.node-teaser

          
      
  /*-------------------------
  /*  Page node
  /*-------------------------
  &.node-page
        



/**************************
/*  MULTI BLOG NODES + ENTITIES
/**************************/ 

.ds-2col

  .group-left,
  .group-right
    clear: none
    
  /* links social + others    
  ul.links
    width: 100%
    
    li
      
      &.facebook,
      &.twitter,
      &.google
        float: right
        margin-right: 5px
        padding: 0 0.5em
        width: auto
        
      &.facebook

      &.twitter
        
      &.google
      
      &.node-readmore
        display: block
        
  /* facebook
  .field-name-reach-social-blocks-fbcomments
    display: block
      
  /* post date 
  .field-name-post-date
    +block-link(auto, auto, 18px, false)
    line-height: 18px
        
  /*-------------------------
  /*  Multi Blog nodes with styled teasers
  /*-------------------------
  &.node-blog,
  &.node-video,
  &.node-media-gallery
    .group-left
      display: block
      margin-right: 24px
      width: 160px
  
    .group-right
      width: 528px 
  
    /* teasers
    &.node-teaser
      .group-left
        padding-top: 40px
        margin-left: 92px
        width: 68px
        
      /* Galleria width issue
      .field-name-reach-media-galleria-slideshow 
        .views-slideshow-galleria-1,
        .views_slideshow_galleria_main,
        .views-slideshow-galleria-images-1,
        .galleria-container
          width: 528px
      
      .field-name-day-display
        .field-item
          +block-link(auto, auto, 30px)
          color: $text_color_alt
          padding-top: 65px
          text-align: center
        
   
      &.node-blog
        .field-name-day-display
          +icons-sprite(blog_tag_grey, false, center-x(68px, blog_tag_grey))
            
      &.node-video
        .field-name-day-display
          +icons-sprite(video_tag_grey, false, center-x(68px, video_tag_grey))
          
      &.node-media-gallery
        .field-name-day-display
          +icons-sprite(image_tag_grey, false, center-x(68px, image_tag_grey))

/* 3 column layout
.ds-3col
  
  .group-left,
  .group-middle,
  .group-right
    clear: none
    padding-top: 0

  .group-left
    padding-right: 24px
    width: 160px
    
  .group-middle
    width: 528px
    
  .group-right
    margin-left: 24px
    width: 160px
    
  &.user-profile
    .field-name-field-bio
      padding-right: 20px
      
.media-gallery-detail-wrapper
  .media-gallery-detail
    img
      float: right
      width: 528px
      
    .media-gallery-detail-info
      clear: none
      float: left
      margin-right: 24px
      margin-top: 0
      width: 160px
      
      .media-gallery-detail-image-info-wrapper,
      .media-gallery-back-link,
      .media-gallery-image-count,
        text-align: left
        display: block
        width: 100%
        
      .media-gallery-back-link,
      .media-gallery-detail-image-info-wrapper
        margin-top: 25px
        
        
      .media-license 
        display: none
        
      .media-gallery-image-count,
      .media-gallery-controls
        display: inline
        
      .media-license
        position: relative

  
  .no-overflow
    float: right
    width: 528px
    
    .field-name-media-description 
      margin-bottom: 35px
    
        
      
        
  
/**************************
/*  VIEWS
/**************************/    
.view
  &.view-content-authors
    .views-row
     
    /* blog listing
  &.view-multi-content-blog
    .pager-load-more
      a
        +buttonize-link(160px, 50px, 30px)
        margin: 0 0 0 368px
        
  /* authors page listing
  &.view-content-authors     
    .views-row
      padding-top: 0
      border-bottom: 0
      
      .user-profile
        padding-bottom: 40px
        
        .group-left,
        .group-middle,
          padding-top: 40px
          border-top: 2px solid #d9d8d1
          border-bottom: 0
          
        .group-right
          padding-top: 40px
      
      &.views-row-first
        .user-profile
          .group-left,
          .group-middle,
          .group-right
            border-top: 0
            padding-top: 20px
        
      &.views-row-last
        .user-profile


/**************************
/*  BLOCKS
/**************************/

.block

  /*-------------------------
  /*  Header image block
  /*-------------------------
  &.block-random-banner-block
    height: $zone_branding_wrapper_h + 17
    overflow: hidden
    right: 0
    
    .views-field-field-banner-images
      img
        max-height: none

  /*-------------------------
  /*  Custom Blocks
  /*-------------------------
  &.block-block    
    /* you belong here text
    &#block-block-3
      .content
        font-size: 72px
        line-height: 80px
    
    /* site by info    
    &.block-block-2
      float: right

  /*-------------------------
  /*  Button Blocks
  /*-------------------------
  &.block-bean
    // bean buttons
    .bean-button-block
      a
        +buttonize-link(160px, 50px, 30px)
        
        /* donate and register    
    &.block-donate-button,
    &.block-register-button
      margin: 0
      width: auto
      
      /* branding
      &.block-region-branding
        margin-top: 0
        position: absolute
        right: 22px
        
        .bean-button-block
          a
            width: 160px
      
        &.block-bean-donate-button
          top: 40px
      
        &.block-bean-register-button
          top: 100px
      
      /* postcript 
      &.block-region-postscript-second
        .bean-button-block
          a
            width: 252px
          
        &.block-register-button
          margin-left: 206px
          margin-right: 24px
        
          
    &.block-bean-site-copyright-info
      float: left
      padding-top: 0
      margin-bottom: 0
      
      .block-inner
        padding-right: 0
      
        
  /*-------------------------
  /*  Blocks in preface
  /*-------------------------
  &.block-region-preface-first,
  &.block-region-preface-second
    width: 260px
    
    .block-inner
      padding-left: 24px
      position: relative
      
    a.exterior-button
      display: block
      position: absolute
      text-indent: -9000px
      top: 0
  
  /* Twitter
  &.block-region-preface-first
    float: right
    padding-left: icons-sprite-width(twitter_out)
    
    a.exterior-button
      +icons-sprite(twitter_out, true)
      left:  -(icons-sprite-width(twitter_out))
      
      &:hover
        +icons-sprite(twitter_over)
      
      
  /* Facebook
  &.block-region-preface-second 
    float: left
    padding-left: icons-sprite-width(facebook_out)
    
    a.exterior-button
      +icons-sprite(facebook_out, true)
      left: -(icons-sprite-width(facebook_out))
      
      &:hover
        +icons-sprite(facebook_over)
        
  /*-------------------------
  /*  Blocks in footer
  /*-------------------------
  &.block-region-footer-second
    .content
      line-height: 40px
    
  /*-------------------------
  /*  Blog exposed filter
  /*-------------------------
  
  &.block-views-exp-multi-content-blog-page-5
    form
      .views-exposed-widget
        float: none
        margin-bottom: 7px
        
        label
          display: none
          
        .form-submit
          margin-top: 0
          
        .form-type-select
          width: 128px
          
          div.selector
            width: 118px
            
          span
            width: 93px
            
          select.uniform-processed
            min-width: none!important
          
      
      #edit-type-wrapper
        display: none
        
  
  /*-------------------------
  /*  Blog filter block
  /*-------------------------
  &.block-reach-blog-filters
    margin: 0 auto
    width: 730px
    
    ul
      
      a
        +block-link(180px, 50px, 24px)
        line-height: 52px
        color: $blue
        text-indent: 33px
        
        &.current
          color: $text_color_alt
        
        // all
        &.view-type-All
          background-image: none
          text-align: center
          text-indent: 0
        
        // blog
        &.view-type-blog
          +icons-sprite(blog_tab, false, 45px, center-y(50px, blog_tab))
          
        // gallery
        &.view-type-media_gallery
          +icons-sprite(image_tab, false, 38px, center-y(50px, image_tab))
        
        // video 
        &.view-type-video
          +icons-sprite(video_tab, false, 34px, center-y(50px, video_tab))
        
    
         
/**************************
/*  FRONT PAGE
/**************************/

body.front
  
  #section-content
  
    #zone-preface
      padding-top: 20px
      
      #region-preface-third
        padding-top: 15px


    #zone-content-wrapper   
      padding-top: 60px

    #zone-content
      
      /*  Sidebar First Region
      /*.........................
      #region-sidebar-first

      
      /*  Content Region
      /*.........................
      #region-content            
        .tabs
          height: 0
          margin: 0
            
      /*  Sidebar Second Region
      /*.........................
      #region-sidebar-second
      
        &.grid-3
          .region-inner
            padding-left: 18px
        

